{
  "name": "haibun-load-tests",
  "type": "module",
  "devDependencies": {
    "@haibun/cli": "1.32.6",
    "@haibun/domain-storage": "1.32.6",
    "@haibun/domain-webpage": "1.32.6",
    "@haibun/out-review": "1.32.6",
    "@haibun/storage-fs": "1.32.6",
    "@haibun/web-playwright": "1.32.6",
    "@haibun/web-server-express": "1.32.6",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0",
    "eslint": "^8.31.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prefer-arrow": "^1.2.2",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.3.1",
    "prettier": "^2.8.1",
    "ts-jest": "^29.0.3",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@haibun/core": "1.32.6",
    "concurrently": "^8.2.2"
  },
  "scripts": {
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "local": "concurrently \"npm run local-dispatcher\" \"npm run local-client\"",
    "local-50": "concurrently \"npm run local-50-dispatcher\" \"npm run local-5-clients\"",
    "local-100": "concurrently \"npm run local-100-dispatcher\" \"npm run local-20-clients\"",
    "local-client": "HAIBUN_O_WEBPLAYWRIGHT_STORAGE=StorageFS HAIBUN_O_HAIBUNLOADTESTSSTEPPER_TOKEN=local92 HAIBUN_O_HAIBUNLOADTESTSSTEPPER_TRACKS_STORAGE=StorageFS HAIBUN_TRACE=true HAIBUN_O_OUTREVIEWS_STORAGE=StorageFS haibun-cli local-tests/client",
    "local-5-clients": "HAIBUN_O_WEBPLAYWRIGHT_STORAGE=StorageFS concurrently \"npm run local-client\" \"npm run local-client\" \"npm run local-client\" \"npm run local-client\" \"npm run local-client\" \"npm run local-client\"",
    "local-20-clients": "HAIBUN_O_WEBPLAYWRIGHT_STORAGE=StorageFS concurrently \"npm run local-5-clients\" \"npm run local-5-clients\" \"npm run local-5-clients\" \"npm run local-5-clients\" \"npm run local-5-clients\"",
    "local-100-clients": "HAIBUN_O_WEBPLAYWRIGHT_STORAGE=StorageFS concurrently \"npm run local-20-clients\" \"npm run local-20-clients\" \"npm run local-20-clients\" \"npm run local-20-clients\" \"npm run local-20-clients\"",
    "local-dispatcher": "HAIBUN_O_HAIBUNLOADTESTSSTEPPER_NUM_TESTS=1 HAIBUN_O_HAIBUNLOADTESTSSTEPPER_TOKEN=local92 HAIBUN_O_HAIBUNLOADTESTSSTEPPER_STORAGE=StorageFS HAIBUN_KEY=__test HAIBUN_O_OUTREVIEWS_STORAGE=StorageFS HAIBUN_TRACE=true haibun-cli local-tests/dispatcher",
    "local-10-dispatcher": "HAIBUN_O_HAIBUNLOADTESTSSTEPPER_NUM_TESTS=10 HAIBUN_O_HAIBUNLOADTESTSSTEPPER_TOKEN=local92 HAIBUN_O_HAIBUNLOADTESTSSTEPPER_STORAGE=StorageFS HAIBUN_KEY=__test HAIBUN_O_OUTREVIEWS_STORAGE=StorageFS HAIBUN_TRACE=true haibun-cli local-tests/dispatcher",
    "local-50-dispatcher": "HAIBUN_O_HAIBUNLOADTESTSSTEPPER_NUM_TESTS=50 HAIBUN_O_HAIBUNLOADTESTSSTEPPER_TOKEN=local92 HAIBUN_O_HAIBUNLOADTESTSSTEPPER_STORAGE=StorageFS HAIBUN_KEY=__test HAIBUN_O_OUTREVIEWS_STORAGE=StorageFS HAIBUN_TRACE=true haibun-cli local-tests/dispatcher",
    "local-100-dispatcher": "HAIBUN_O_HAIBUNLOADTESTSSTEPPER_NUM_TESTS=100 HAIBUN_O_HAIBUNLOADTESTSSTEPPER_TOKEN=local92 HAIBUN_O_HAIBUNLOADTESTSSTEPPER_STORAGE=StorageFS HAIBUN_KEY=__test HAIBUN_O_OUTREVIEWS_STORAGE=StorageFS HAIBUN_TRACE=true haibun-cli local-tests/dispatcher",
    "local-500-dispatcher": "HAIBUN_O_HAIBUNLOADTESTSSTEPPER_NUM_TESTS=500 HAIBUN_O_HAIBUNLOADTESTSSTEPPER_TOKEN=local92 HAIBUN_O_HAIBUNLOADTESTSSTEPPER_STORAGE=StorageFS HAIBUN_KEY=__test HAIBUN_O_OUTREVIEWS_STORAGE=StorageFS HAIBUN_TRACE=true haibun-cli local-tests/dispatcher",
    "local-5000-dispatcher": "HAIBUN_O_HAIBUNLOADTESTSSTEPPER_NUM_TESTS=5000 HAIBUN_O_HAIBUNLOADTESTSSTEPPER_TOKEN=local92 HAIBUN_O_HAIBUNLOADTESTSSTEPPER_STORAGE=StorageFS HAIBUN_KEY=__test HAIBUN_O_OUTREVIEWS_STORAGE=StorageFS HAIBUN_TRACE=true haibun-cli local-tests/dispatcher",
    "test-watch": "NODE_OPTIONS=--experimental-vm-modules jest --watch",
    "publish": "HAIBUN_KEY=__publish HAIBUN_O_OUTREVIEWS_PUBLISH_ROOT=./files/published HAIBUN_O_OUTREVIEWS_STORAGE=StorageFS HAIBUN_O_OUTREVIEWS_PUBLISH_STORAGE=StorageFS haibun-cli --config publish/local publish",
    "build": "tsc",
    "build-watch": "tsc -w",
    "lint": "lint --ext .ts ./src/"
  },
  "version": "1.32.6"
}